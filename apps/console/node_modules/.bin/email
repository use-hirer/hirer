#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/dist/source/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/dist/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/dist/source/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/dist/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules/react-email/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/react-email@1.10.1/node_modules:/Users/nickmandylas/Projects/hirer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/source/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/source/index.js" "$@"
fi
